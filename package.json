{
  "name": "fhevm-example-hub",
  "version": "1.0.0",
  "description": "Comprehensive FHEVM examples generator with automated documentation",
  "main": "index.js",
  "scripts": {
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "postcompile": "npm run typechain",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat test",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "create-example": "ts-node scripts/create-fhevm-example.ts",
    "create-category": "ts-node scripts/create-fhevm-category.ts",
    "generate-docs": "ts-node scripts/generate-docs.ts",
    "generate-all-docs": "ts-node scripts/generate-docs.ts --all",
    "help:category": "echo 'Available categories: basic, auctions, openzeppelin, games'"
  },
  "keywords": [
    "fhevm",
    "zama",
    "fhe",
    "privacy",
    "blockchain",
    "examples"
  ],
  "author": "Atlas",
  "license": "BSD-3-Clause-Clear",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@fhevm/mock-utils": "^0.3.0-4",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-ignition": "^0.15.6",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=18.0.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "ethers": "^6.13.4",
    "globals": "^15.13.0",
    "hardhat": "^2.22.18",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.10",
    "solidity-coverage": "^0.8.14",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.2",
    "typescript": ">=4.5.0",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "@fhevm/hardhat-plugin": "^0.3.0-4",
    "@fhevm/solidity": "^0.10.0",
    "@openzeppelin/contracts": "^5.1.0",
    "@zama-fhe/relayer-sdk": "^0.3.0-8",
    "fhevmjs": "^0.6.2"
  }
}
